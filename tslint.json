{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "max-line-length": [
      true, 
      140
    ],
    "ordered-imports": false,
    "quotemark": [
      true,
      "single"
    ],
    "trailing-comma": [
      true,
      {"multiline": "never", "singleline": "never"}
    ],
    "no-console": false,
    "no-unused-expression": true,
    "no-unused-variable": [
      true, 
      "check-parameters"
    ]
  }
}