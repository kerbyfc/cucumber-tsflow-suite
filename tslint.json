{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "max-line-length": [
      true, 
      140
    ],
    "quotemark": [
      true,
      "single"
    ],
    "no-console": false,
    "no-unused-expression": true,
    "no-unused-variable": [
      true, 
      "check-parameters"
    ]
  }
}